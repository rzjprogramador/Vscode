// Place your key bindings in this file to override the defaultsauto[]
// tester

[
	{
		"key": "f1",
		"command": "workbench.action.closeEditorsInGroup",
		"args": "fecha_todas_janelas_de_editores"
	},
	{
		"key": "f3",
		"command": "workbench.action.closeActiveEditor",
		"args": "fecha_autal_janela_de_editor"
	},
	{
		"key": "f4",
		"command": "workbench.action.closeOtherEditors",
		"args": "fechar_outras_janelas_de_editor"
	},
	{
		"key": "f12",
		"command": "workbench.action.terminal.toggleTerminal",
		"when": "terminal.active",
		"args": "fecha_e_abre_toggle_TERMINAL"
	},
	{
		"key": "f5",
		"command": "code-runner.run",
		"args": "roda_arquivos_console_via_coderunner"
	},
	{
		"key": "ctrl+\\",
		"command": "editor.action.commentLine",
		"when": "editorTextFocus && !editorReadonly",
		"args": "foo"
	},
	{
		"key": "ctrl+shift+\\",
		"command": "editor.action.blockComment",
		"when": "editorTextFocus && !editorReadonly",
		"args": "foo"
	},
	{
		"key": "ctrl+d",
		"command": "editor.action.deleteLines",
		"when": "textInputFocus && !editorReadonly",
		"args": "foo"
	},
	{
		"key": "ctrl+f5",
		"command": "workbench.action.terminal.focusAtIndex5",
		"args": "foo"
	},
	{
		"key": "ctrl+shift+alt+f1",
		"command": "workbench.action.terminal.kill",
		"args": "foo"
	},
	{
		"key": "alt+,",
		"command": "editor.action.addSelectionToNextFindMatch",
		"when": "editorFocus",
		"args": "foo"
	},
	{
		"key": "alt+.",
		"command": "editor.action.selectHighlights",
		"when": "editorFocus",
		"args": "foo"
	},
	{
		"key": "ctrl+w",
		"command": "-workbench.action.closeGroup",
		"when": "activeEditorGroupEmpty && multipleEditorGroups",
		"args": "foo"
	},
	{
		"key": "ctrl+w",
		"command": "-workbench.action.terminal.killEditor",
		"when": "terminalEditorFocus && terminalFocus && terminalHasBeenCreated && resourceScheme == 'vscode-terminal' || terminalEditorFocus && terminalFocus && terminalProcessSupported && resourceScheme == 'vscode-terminal'",
		"args": "foo"
	},

	{
		"key": "ctrl+k w",
		"command": "-workbench.action.closeEditorsInGroup",
		"args": "foo"
	},
	{
		"key": "ctrl+t",
		"command": "workbench.action.togglePanel",
		"args": "foo"
	},
	{
		"key": "ctrl+j",
		"command": "-workbench.action.togglePanel",
		"args": "foo"
	},
	{
		"key": "ctrl+e",
		"command": "workbench.action.toggleSidebarVisibility",
		"args": "foo"
	},
	{
		"key": "ctrl+b",
		"command": "-workbench.action.toggleSidebarVisibility",
		"args": "foo"
	},
	{
		"key": "alt+\\",
		"command": "workbench.action.splitEditor",
		"args": "foo"
	},
	{
		"key": "ctrl+\\",
		"command": "-workbench.action.splitEditor",
		"args": "foo"
	},
	{
		"key": "shift+f1 ctrl+shift",
		"command": "actions.find",
		"when": "editorFocus || editorIsOpen",
		"args": "foo"
	},
	{
		"key": "ctrl+shift+alt+n",
		"command": "workbench.action.newWindow",
		"args": "foo"
	},
	{
		"key": "ctrl+shift+n",
		"command": "-workbench.action.newWindow",
		"args": "foo"
	},
	{
		"key": "ctrl+shift+l",
		"command": "-editor.action.selectHighlights",
		"when": "editorFocus",
		"args": "foo"
	},
	{
		"key": "shift+home shift+home",
		"command": "interactive.scrollToTop",
		"when": "resourceScheme == 'vscode-interactive'",
		"args": "foo"
	},
	{
		"key": "ctrl+home",
		"command": "-interactive.scrollToTop",
		"when": "resourceScheme == 'vscode-interactive'",
		"args": "foo"
	},
	{
		"key": "shift+home shift+home",
		"command": "cursorTop",
		"when": "textInputFocus",
		"args": "foo"
	},
	{
		"key": "ctrl+home",
		"command": "-cursorTop",
		"when": "textInputFocus",
		"args": "foo"
	},
	{
		"key": "shift+home shift+home",
		"command": "notebook.focusTop",
		"when": "notebookEditorFocused && !inputFocus",
		"args": "foo"
	},
	{
		"key": "ctrl+home",
		"command": "-notebook.focusTop",
		"when": "notebookEditorFocused && !inputFocus",
		"args": "foo"
	},
	{
		"key": "shift+home shift+home",
		"command": "cursorTopSelect",
		"when": "textInputFocus",
		"args": "foo"
	},
	{
		"key": "ctrl+shift+home",
		"command": "-cursorTopSelect",
		"when": "textInputFocus",
		"args": "foo"
	},
	{
		"key": "shift+f1",
		"command": "workbench.action.showCommands",
		"args": "foo"
	},
	{
		"key": "f1",
		"command": "-workbench.action.showCommands",
		"args": "foo"
	},
	{
		"key": "ctrl+shift+down",
		"command": "editor.action.copyLinesDownAction",
		"when": "editorTextFocus && !editorReadonly",
		"args": "foo"
	},
	{
		"key": "ctrl+shift+up",
		"command": "editor.action.copyLinesUpAction",
		"when": "editorTextFocus && !editorReadonly",
		"args": "foo"
	},
	{
		"key": "ctrl+shift+alt+down",
		"command": "-editor.action.copyLinesDownAction",
		"when": "editorTextFocus && !editorReadonly",
		"args": "foo"
	},
	{
		"key": "ctrl+shift+alt+up",
		"command": "-editor.action.copyLinesUpAction",
		"when": "editorTextFocus && !editorReadonly",
		"args": "foo"
	},
	{
		"key": "ctrl+shift+a",
		"command": "-editor.action.blockComment",
		"when": "editorTextFocus && !editorReadonly",
		"args": "foo"
	}
]
