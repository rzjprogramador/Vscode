[
  {
    "key": "tab",
    "command": "tab",
    "when": "editorTextFocus && !editorTabMovesFocus",
    "args": "Tab identa"
  },
  {
      "key": "shift+tab",
      "command": "outdent",
      "when": "editorTextFocus && !editorTabMovesFocus",
      "args": "re-Tab desindenta."
  },
  {
    "key": "f4",
    "command": "code-runner.run",
   "args": "run code arquivo no terminal - - default era ctrl+alt+n"
  },
  {
    "key": "ctrl+f",
    "command": "actions.find",
    "when": "editorFocus || editorIsOpen",
    "args": "find crtl+f - caso nao tiver habilitado no vscode_vim add este aqui."
  },
  {
    "key": "ctrl+c",
    "command": "editor.action.clipboardCopyAction",
    "args": "copiar - reconfigurando ctrl+c de novo vscode_vim"
  },
  {
    "key": "ctrl+c",
    "command": "workbench.action.terminal.copySelection",
    "when": "terminalFocus && terminalHasBeenCreated && terminalTextSelected || terminalFocus && terminalProcessSupported && terminalTextSelected",
    "args": "copiar do terminal em focus"
  },
  {
    "key": "ctrl+c",
    "command": "filesExplorer.copy",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !inputFocus",
    "args": "copiar em explorer arquivos no sidebar"
  },
  {
    "key": "ctrl+x",
    "command": "filesExplorer.cut",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus",
    "args": "recortar em explorer arquivos no sidebar"
  },
  {
  "key": "ctrl+v",
  "command": "editor.action.clipboardPasteAction",
  "args": "colar - reconfigurando ctrl+v de novo vscode_vim"
  },
  {
    "key": "ctrl+v",
    "command": "filesExplorer.paste",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceReadonly && !inputFocus",
    "args": "colar em explorer arquivos no sidebar"
  },
  {
    "key": "ctrl+v",
    "command": "workbench.action.terminal.paste",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported",
    "args": "colar em terminal em focus"
  },
  {
    "key": "ctrl+v",
    "command": "workbench.action.terminal.paste",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported",
    "args": "colar em terminal em focus"
  },
  {
    "key": "ctrl+x",
    "command": "editor.action.clipboardCutAction",
    "args": "recortar - default: ctrl+x"
  },
  {
    "key": "ctrl+a",
    "command": "editor.action.selectAll",
    "args": "seleciona tudo - default: ctrl+a"
  },
	{
		"key": "alt+,",
		"command": "editor.action.addSelectionToNextFindMatch",
		"when": "editorFocus",
		"args": "ocorrencia_atual_UMA - default era ctrl+d"
	},
	{
		"key": "alt+.",
		"command": "editor.action.selectHighlights",
		"when": "editorFocus",
		"args": "ocorrencias_TODAS - default era ctrl+."
	},
  {
    "key": "ctrl+.",
    "command": "editor.action.quickFix",
    "when": "editorHasCodeActionsProvider && textInputFocus && !editorReadonly",
    "args": "sugestao de conserto rapido - quick fix - default ???"
  },
  {
    "key": "ctrl+shift+down",
    "command": "editor.action.insertCursorBelow",
    "when": "editorTextFocus",
    "args": "inserir cursor linhas abaixo - default -> ctrl+shift+down"
  },
	{
		"key": "ctrl+d",
		"command": "editor.action.deleteLines",
		"when": "textInputFocus && !editorReadonly",
		"args": "deleta linha -> default ctrl+shift+k"
	},
	{
		"key": "ctrl+\\",
		"command": "editor.action.commentLine",
		"when": "editorTextFocus && !editorReadonly",
		"args": "comentario em linha -> default ctrl+k+ctrl+c"
	},
	{
		"key": "ctrl+shift+\\",
		"command": "editor.action.blockComment",
		"when": "editorTextFocus && !editorReadonly",
		"args": "comentario em bloco -> default ctrl+shift+a"
  },
  {
		"key": "ctrl+w",
		"command": "workbench.action.closeActiveEditor",
		"args": "fecha atual janela ativa -> default ctrl+w"
	},
  {
    "key": "ctrl+q",
    "command": "workbench.action.closeAllEditors",
    "args": "fecha todas janelas ativa forcado force -> default: ctrl+k ctrl+w"
  },
  {
    "key": "ctrl+tab",
    "command": "workbench.action.quickOpenNavigateNextInEditorPicker",
    "when": "inEditorsPicker && inQuickOpen",
    "args": "navega entre janelas abertas - default -> ctrl+tab"
  },
	{
		"key": "f1",
		"command": "workbench.action.terminal.toggleTerminal",
		"when": "terminal.active",
		"args": "toggle TERMINAL - default: ctrl+shift+`<craze>"
	},
  {
    "key": "shift+f1",
    "command": "workbench.action.terminal.kill",
    "args": "fecha kill terminal em foco - default: #NaoTinha"
  },
  {
    "key": "ctrl+enter",
    "command": "breadcrumbs.revealFocusedFromTreeAside",
    "when": "breadcrumbsActive && breadcrumbsVisible && listFocus && !inputFocus",
    "args": "abrir arquivo selecionado em vsplit -> default: ctrl+enter"
  },
	{
		"key": "ctrl+e",
		"command": "workbench.action.toggleSidebarVisibility",
		"args": "toggle_explorador_de_arquivos - default: ctrl+b"
	},
  {
    "key": "f2",
    "command": "editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly",
    "args": "renomea definicao variavel - rename symbol -> default: f2"
  },
	{
		"key": "ctrl+f1",
		"command": "workbench.action.showCommands",
		"args": "mostra comandos - default: f1"
	},
  {
		"key": "ctrl+down",
		"command": "editor.action.copyLinesDownAction",
		"when": "editorTextFocus && !editorReadonly",
		"args": "duplica > copia e cola selecionado linha abaixo - Copy Line Down -- default ctrl+shift+Alt+down - obs: feito somente para baixo - se precisar para cima esta nos backups."
	},
  {
    "key": "ctrl+l",
    "command": "expandLineSelection",
    "when": "textInputFocus",
    "args": "seleciona linha e expand para baixo - default: ctrl+l"
  },
  {
    "key": "ctrl+k ctrl+f",
    "command": "editor.action.formatSelection",
    "when": "editorHasDocumentSelectionFormattingProvider && editorTextFocus && !editorReadonly",
    "args": "formatar documento -#TODO configurar_formatador - default: ctrl+k ctrl+f"
  },
  {
    "key": "ctrl+.",
    "command": "editor.action.indentLines",
    "when": "editorTextFocus && !editorReadonly",
    "args": "identa linha a frente - default: ctrl+]"
  },
  {
    "key": "ctrl+,",
    "command": "editor.action.outdentLines",
    "when": "editorTextFocus && !editorReadonly",
    "args": "desidenta linha a atraz - default: ctrl+["
  },
  {
    "key": "shift+f1",
    "command": "workbench.action.showCommands",
    "args": "mostra todos comandos - show all commands"
  },
  {
    "key": "alt+left",
    "command": "workbench.action.terminal.resizePaneLeft",
    "when": "editorFocus || terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported",
    "args": "redimensiona terminal a direita -> default: ctrl+shift+left"
  },
  {
    "key": "alt+right",
    "command": "workbench.action.terminal.resizePaneRight",
    "when": "editorFocus || terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported",
    "args": "redimensiona terminal a esquerda -> default: ctrl+shift+right"
  }




]
