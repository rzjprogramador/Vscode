{
  "imports": {
    "std/": "https://deno.land/std@0.181.0/"
  },
  "tasks": {
    "run": "deno run -A --watch",
    "test": "deno test -A --watch"
  },
  "fmt": {
    "options": {
      "useTabs": true,
      "lineWidth": 120,
      "indentWidth": 2,
      "semiColons": false,
